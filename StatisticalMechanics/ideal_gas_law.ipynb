   "source": [
    "## Ideal Gas Law\n",
    "\n",
    "\n",
    "$$ \\large PV \\ = \\ nRT \\ = \\ n k_B T $$\n",
    "\n",
    "\n",
    "where\n",
    "\n",
    "\n",
    "- $P$: pressure\n",
    "\n",
    "\n",
    "- $V$: volume\n",
    "\n",
    "\n",
    "- $n$: amount of substance\n",
    "\n",
    "\n",
    "- $R$: ideal gas constant\n",
    "\n",
    "\n",
    "- $T$: temperature\n",
    "\n",
    "\n",
    "- $k_B$: Boltzmann constant"
   ]
  }
 ],
